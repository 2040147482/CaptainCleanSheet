# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-publishable-or-anon-key

# Plugin auth & permissions
PLUGIN_JWT_SECRET=replace-with-a-strong-random-secret
PLUGIN_TOKEN_TTL_MINUTES=15
PLUGIN_JWT_TTL_SECONDS=3600

# Creem (billing)
CREEM_API_KEY=replace-with-your-creem-api-key
CREEM_WEBHOOK_SECRET=replace-with-your-creem-webhook-secret
